{
  "name": "dobem",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run start:prisma && next build",
    "start": "next start",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x} --cache --max-warnings=0",
    "lint:fix": "eslint src/**/*.ts{,x} --fix",
    "migrate:dev": "npx dotenv -e .env.local -- prisma migrate dev",
    "prisma:studio": "npx dotenv -e .env.local -- prisma studio",
    "prisma:seed": "npx dotenv -e .env.local -- prisma db seed",
    "start:prisma": "docker compose up -d && npm run migrate:dev && npm run prisma:seed && npm run prisma:studio"
  },
  "prisma": {
    "seed": "node ./prisma/seed.mjs"
  },
  "dependencies": {
    "@prisma/client": "~5.2.0",
    "@types/node": "~20.5.0",
    "@types/react": "~18.2.20",
    "@types/react-dom": "~18.2.7",
    "axios": "~1.5.0",
    "bcrypt": "~5.1.1",
    "dayjs": "~1.11.9",
    "dotenv-cli": "~7.3.0",
    "jose": "~4.14.6",
    "jsonwebtoken": "~9.0.2",
    "nanoid": "^5.0.2",
    "next": "^13.5.6",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "react-modal": "^3.16.1",
    "text-encoding": "~0.7.0",
    "typescript": "~5.1.6",
    "yup": "~1.2.0"
  },
  "devDependencies": {
    "@types/bcrypt": "~5.0.0",
    "@types/jsonwebtoken": "~9.0.2",
    "@types/nanoid": "^3.0.0",
    "@types/react-modal": "^3.16.2",
    "@types/text-encoding": "~0.0.36",
    "@typescript-eslint/eslint-plugin": "~5.62.0",
    "@typescript-eslint/parser": "~5.62.0",
    "eslint": "~8.47.0",
    "eslint-config-airbnb": "~19.0.4",
    "eslint-config-airbnb-typescript": "~17.1.0",
    "eslint-config-prettier": "~8.10.0",
    "eslint-import-resolver-typescript": "~3.6.0",
    "eslint-plugin-import": "~2.28.1",
    "eslint-plugin-jsx-a11y": "~6.7.1",
    "eslint-plugin-prettier": "~4.2.1",
    "eslint-plugin-react": "~7.33.2",
    "eslint-plugin-react-hooks": "~4.6.0",
    "prettier": "~2.8.8",
    "prisma": "~5.2.0"
  }
}
